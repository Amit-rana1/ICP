class Solution {
    public int uniquePaths(int m, int n) {
        return helper(0,0,m-1,n-1,new int[m][n]);
    }
    public int helper(int cr,int cc,int er,int ec,int[][] dp){
        if(cr==er && cc==ec) return 1;
        if(cr>er || cc>ec) return 0;
        if(dp[cr][cc]!=0) return dp[cr][cc];
        int h=helper(cr,cc+1,er,ec,dp);
        int v=helper(cr+1,cc,er,ec,dp);
        dp[cr][cc]=h+v;
        return dp[cr][cc];
    }
}
